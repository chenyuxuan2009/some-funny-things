@echo off
setlocal enabledelayedexpansion
title 转换颜色

echo  0 = 黑色 		
echo  1 = 蓝色       
echo  2 = 绿色       
echo  3 = 浅绿色     
echo  4 = 红色       
echo  5 = 紫色       
echo  6 = 黄色       
echo  7 = 白色       
echo  8 = 灰色
echo  9 = 淡蓝色
echo 10 = 淡绿色
echo 11 = 淡浅绿色
echo 12 = 淡红色
echo 13 = 淡紫色
echo 14 = 淡黄色
echo 15 = 亮白色
echo 注：如果背景和字体同色会显示不出来哦
:start
set /p a=你想转换的背景编号：
set /p b=你想转换的字体编号：
if %a% EQU 0 (
    if %b% EQU 0 (
        color 00
        goto start
    )
    if %b% EQU 1 (
        color 01
        goto start
    )
    if %b% EQU 2 (
